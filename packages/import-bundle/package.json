{
  "name": "@agoric/import-bundle",
  "version": "0.2.22",
  "description": "load modules created by @agoric/bundle-source",
  "parsers": {
    "js": "mjs"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "test": "ava",
    "test:xs": "exit 0",
    "build": "exit 0",
    "lint-check": "yarn lint",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint": "eslint '**/*.js'"
  },
  "dependencies": {
    "@agoric/assert": "^0.3.6",
    "@endo/base64": "^0.2.5",
    "@endo/compartment-mapper": "^0.5.0"
  },
  "devDependencies": {
    "@agoric/babel-standalone": "^7.14.3",
    "@agoric/bundle-source": "^1.4.4",
    "@agoric/install-ses": "^0.5.20",
    "@agoric/swingset-vat": "^0.18.6",
    "@endo/ses-ava": "^0.2.5",
    "ava": "^3.12.1",
    "esm": "agoric-labs/esm#Agoric-built"
  },
  "files": [
    "README.md",
    "LICENSE",
    "NEWS.md",
    "src/**/*.js"
  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk/packages/import-bundle",
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "2m"
  }
}
